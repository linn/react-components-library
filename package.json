{
  "name": "@linn-it/linn-form-components-library",
  "version": "1.0.17",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.2",
    "react-select": "^2.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "jest src/ && rollup -c -o dist/bundle.min.js",
    "deploy": "npm run build && npm publish && npm run deploy-storybook",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages -- --out=.out",
    "test:generate-output": "jest --json --outputFile=jest-test-results.json src/components/__tests__/*",
    "test": "jest src/",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-jest": "^4.1.1",
    "@storybook/addon-knobs": "^4.1.1",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addon-options": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "jest": "^23.6.0",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-uglify": "^6.0.0",
    "storybook-react-router": "^1.0.2"
  },
  "main": "dist/bundle.min.js"
}
